{
  "name": "perfanalytics",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": { 
    "setup": "npm i && cd dashboard && npm i",
    "start": "NODE_ENV=development && cd dashboard && npm run build && cd .. && node index.js",
    "start:dev:be": "node index.js",
    "start:dev:fe": "cd dashboard; npm run serve",
    "lint": "eslint ./src",
    "test:be": "clear; jest --verbose",
    "test:fe": "cd dashboard && npm run test:unit",
    "coverage:be": "clear; jest --verbose --coverage",
    "coverage:fe": "cd dashboard && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/osmanertem/perfanalytics.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/osmanertem/perfanalytics/issues"
  },
  "homepage": "https://github.com/osmanertem/perfanalytics#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.2.1",
    "mongoose": "^5.10.4",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "eslint": "^7.9.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "modulePathIgnorePatterns": [
      "dashboard"
    ]
  }
}
